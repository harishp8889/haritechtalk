<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="dynamic_textbox.Text" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
			<Panel content="{/texts}">
				<content>
					<Input value="{text}"></Input>
				</content>
			</Panel>
			<Button text="Click Me" press="_onButtonPress"></Button>
		</content>
	</Page>
</core:View>